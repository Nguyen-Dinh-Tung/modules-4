<a href="/create"><div class="btn btn-primary">Thêm mới</div></a>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">STT</th>
      <th scope="col">Tên</th>
      <th scope="col">Tuổi</th>
      <th scope="col">Chức vụ</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach((element , index)=>{%>
    <tr>
      <th scope="row"><%=index +1 %></th>
      <td><%= element.name %></td>
      <td><%= element.age %></td>
      <td><%= element.position %></td>
      <td>
        <a href="/delete?id=<%=element.id%>"
          ><button class="btn btn-danger">Xóa</button></a
        >
      </td>
    </tr>
    <% })%>
  </tbody>
</table>
