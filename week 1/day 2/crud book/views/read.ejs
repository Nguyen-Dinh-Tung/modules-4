<h3>Tủ sách online</h3>
<% data.forEach((element,index)=>{ %>
<p><%= element.name %></p>
<p><%= element.price %></p>
<p><%= element.quantity %></p>
<p><%= element.author %></p>
<a href="/details?id=<%=element.id%>"><button>Details</button></a>
<a href="/delete?id=<%=element.id%>"><button>Delete</button></a>
<%}) %>
